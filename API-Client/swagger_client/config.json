{
	"hpc_abort":"scancel {hpcjobid}",
	"hpc_status":"scontrol show jobid {hpcjobid}",
	"hpc":"sbatch {bashfile}",
	"shell":"chmod +x {shellfile} && {shellfile}",
	"compile":"sbatch --wrap=\"make -C {makefile}\"",
	"archive":"zip -FSr {destination} {source}",
	"unarchive":"unzip {source} -d {destination}",
	"copy":"cp -r {source} {destination}"
}